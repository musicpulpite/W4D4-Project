

<% Band.all.each do |band| %>
  <a class="band-link" href="<%= band_url(band) %>"><%= band.name %></a>
  <br>
<% end %>

<footer>
  <form class="edit_button" action="<%= new_band_url %>" method="get">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input type="submit" name="" value="New Band">
  </form>
</footer>
